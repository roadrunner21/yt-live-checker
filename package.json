{
  "name": "yt-live-checker",
  "version": "1.1.0",
  "description": "A tool to check if a YouTube channel is currently live streaming.",
  "main": "src/index.js",
  "exports": {
    ".": {
      "import": "./src/index.mjs",
      "require": "./src/index.js"
    },
    "./cli": {
      "import": "./src/cli/index.js",
      "require": "./src/cli/index.js"
    }
  },
  "scripts": {
    "start": "node src/cli/index.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "check": "node src/cli/index.js",
    "check:streams": "node src/cli/index.js --streams",
    "check:streams-json": "node src/cli/index.js --streams-json",
    "check:save-html": "node src/cli/index.js --save-html",
    "test": "node --test"
  },
  "keywords": [
    "YouTube",
    "live",
    "stream",
    "channel",
    "status",
    "checker"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/roadrunner21/yt-live-checker.git"
  },
  "author": "Your Name <your.email@example.com>",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.12.2",
    "cheerio": "^1.0.0",
    "debug": "^4.3.7",
    "jsonpath-plus": "^10.3.0",
    "winston": "^3.16.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.38.0",
    "eslint": "^9.38.0",
    "globals": "^15.11.0",
    "nock": "^13.5.6"
  }
}
